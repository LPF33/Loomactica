        "@types/mysql2": "github:types/mysql2",
        "@types/socket.io": "^2.1.11",
        "@types/socket.io-client": "^1.4.34",
        "@types/uuid": "^8.3.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-handlebars": "^5.1.0",
        "mysql2": "^2.2.5",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.1",
